{
  "development": {
    "use_env_variable": "DATABASE_URL",
    "username": "process.env.DATABASE_USER",
    "password": "process.env.DATABASE_PASS",
    "database": "process.env.DATABASE_DATA",
    "host": "process.env.DATABASE_HOST",
    "dialect":"postgres",
    "protocol":"postgres",
    "port": "process.env.DATABASE_PORT", 
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  },
  "local": {
    "username": "postgres",
    "password": "password",
    "database": "wisper-local",
    "host": "localhost",
    "dialect":"postgres",
    "protocol":"postgres",
    "port": "5432"
  },
  "production": {
    "username": "process.env.DATABASE_USER",
    "password": "process.env.DATABASE_PASS",
    "database": "process.env.DATABASE_DATA",
    "host": "process.env.DATABASE_HOST",
    "dialect":"postgres",
    "protocol":"postgres",
    "port": "process.env.DATABASE_PORT", 
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}